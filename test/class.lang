class Person {

    fn init(firstName, lastName) {
        this.firstName = firstName;
        this.lastName = lastName;
    }

    fn getName() {
        return this.firstName + " " + this.lastName;
    }

}

var p1 = Person("Adam", "Jedlička");
var p2 = Person("Franta", "Sádlo");

print p1.getName();
print p2.getName();

p1.getName = p2.getName;

print p1.getName();
